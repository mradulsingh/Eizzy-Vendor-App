<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:complete="false" app:inProgress="true" app:pending="false">
        <shape android:shape="oval">
            <size android:width="@dimen/timeline_milestone_wh" android:height="@dimen/timeline_milestone_wh" />
            <stroke android:width="@dimen/timeline_milestone_indicator_stroke" android:color="@color/indianYellow" />
        </shape>
    </item>
    <item app:complete="false" app:inProgress="false" app:pending="true">
        <shape android:shape="oval">
            <size android:width="@dimen/timeline_milestone_wh" android:height="@dimen/timeline_milestone_wh" />
            <stroke android:width="@dimen/timeline_milestone_indicator_stroke" android:color="@color/indianYellow" />
        </shape>
    </item>
    <item app:complete="true" app:inProgress="false" app:pending="false">
        <shape android:shape="oval">
            <size android:width="@dimen/timeline_milestone_wh" android:height="@dimen/timeline_milestone_wh" />
            <solid android:color="@color/deepSkyBlue" />
        </shape>
    </item>
</selector>
